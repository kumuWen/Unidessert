<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>會員</title>
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/jquery-ui.css">
    <script src="/js/bootstrap.bundle.js"></script>
    <script src="/js/jquery-3.7.0.min.js"></script>
    <script src="/js/jquery-ui.js"></script>
    
</head>


<body>
    <div class="all">
        <%- include('backNav') %>
        <!-- 側邊menu -->
        <div class="content d-flex">
            <div class="container">
                <div class="me-auto">
                    <p class="text-end">
                        user01，您好！|
                        <a href="#">登出</a>
                    </p>
                </div>
                <div class="product d-flex">
                    <p class="fs-4">
                        會員列表
                    </p>
                    
                    <table class="table fs-6 table-hover table-sm">
                        <thead class="text-center table-warning">
                          <tr>
                            <th scope="col">會員編號</th>
                            <th scope="col">會員姓名</th>
                            <th scope="col">電話</th>
                            <th scope="col">email</th>
                            <th scope="col">生日</th>
                            <th scope="col">加入日期</th>
                            <th scope="col">編輯/刪除</th>
                          </tr>
                        </thead>
                        <tbody class="text-center">
                            <%for (let k=0; k < cat.length; k++) { %>
                                <tr>
                                    <th scope="row"><%= cat[k].uid %></th>
                                    <td><%= cat[k].uname %></td>
                                    <td><%= cat[k].umobile %></td>
                                    <td><%= cat[k].uemail %></td>
                                    <td><%= cat[k].ubirth %></td>
                                    <td><%= cat[k].umembertime %></td>
                                    <th ><button><a href="/backOrderEdit">編輯</a></button>
                                        <button><a href="/backOrderEdit">刪除</a></button>
                                    </th>
                                  </tr>
                                <%}%>
                          
                        </tbody>
                      </table>
                </div>
            </div>
        </div>
    </div>
</body>

</html>